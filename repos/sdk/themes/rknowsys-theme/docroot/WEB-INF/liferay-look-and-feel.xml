<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="rknowsys" name="Rknowsys">
		<settings>
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="false"></setting>
			<setting configurable="true" key="show-breadcrumb" type="checkbox" value="true" />
		</settings>
		<color-scheme id="01" name="default">
			<css-class>default</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="02" name="orange">
			<css-class>skin-orange</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="03" name="green">
			<css-class>skin-green</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="04" name="pink">
			<css-class>skin-pink</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="05" name="yellow">
			<css-class>skin-yellow</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
	</theme>
</look-and-feel>